{
    "name": "andreustimm/phpunit-examples",
    "description": "Examples on using PHPUnit",
    "type": "project",
    "require": {
        "phormium/phormium": "~0.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "AndreusTimm\\PHPUnit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AndreusTimm\\PHPUnit\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox --coverage-html tests/reports/coverage/",
        "test-unit": "vendor/bin/phpunit --testsuite unit --testdox --coverage-html tests/reports/coverage/",
        "test-integration": "vendor/bin/phpunit --testsuite integration --testdox --coverage-html tests/reports/coverage/"
    },
    "license": "Apache License 2.0",
    "authors": [
        {
            "name": "Andreus Timm",
            "email": "andreus.timm@gmail.com"
        }
    ]
}
